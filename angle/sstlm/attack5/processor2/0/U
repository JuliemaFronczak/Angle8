FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    frontback
    {
        type            empty;
    }
    topbottom
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }
    walls
    {
        type            noSlip;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


