FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.0223;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.0223;
    }
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            omegaWallFunction;
        value           uniform 0.0223;//
    }
    topbottom
    {
        type            fixedValue;
        value           uniform 0.0223;
    }
    
    frontback
    {
        type            empty;
    }

}


// ************************************************************************* //
