FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];


internalField   uniform 0.00015;

boundaryField
{
    frontback
    {
        type            empty;
    }
    topbottom
    {
        type            fixedValue;
        value           uniform 0.00015;
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.00015;
    }
    walls
    {
        type            kqRWallFunction;
        value           uniform 0.00015;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 0.00015;
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform 0.00015;
    }
}


